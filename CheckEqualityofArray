class Main {
    public static void main(String[] args) {
        int arr[] = {1,2,3,4};
        int arr1[] = {1,2,3,4};
        
        boolean flag = arr.equals(arr1);
        
        if(flag = true)
        {
            System.out.println("Both are equal");
        }
        else
        {
            System.out.println("Both are not equal");
        }
        
    }
}
