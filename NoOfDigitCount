import java.util.Scanner;
public class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter value :");
        int value = sc.nextInt();
        
        
        int count = 0;
        while(value>0)
        {
          value = value/10;
          count++;
        }
        System.out.println("The number of digits " +count);
        
        
    }
}
